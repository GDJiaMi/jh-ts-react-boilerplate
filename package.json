{
  "name": "jh-ts-react-boilerplate",
  "version": "0.2.0",
  "description": "typescript + react + mobx boilerplate",
  "main": "index.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "generate": "plop --plopfile internals/generators/index.js",
    "start": "cross-env NODE_ENV=development node internals/scripts/start.js",
    "build": "cross-env NODE_ENV=production node internals/scripts/build",
    "analyze": "cross-env NODE_ENV=production node internals/scripts/analyze",
    "dll": "cross-env NODE_ENV=development node internals/scripts/dependencies.js",
    "serve": "http-server build -g -o",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:ts": "tslint -c tslint.json \"src/**/*.ts?(x)\"",
    "lint:css": "stylelint \"src/@(components|containers)/**/*.ts?(x)\"",
    "prettier": "prettier --config .prettierrc --write \"src/**/*.ts?(x)\"",
    "precommit": "lint-staged",
    "preinstall": "node internals/scripts/npmcheckversion",
    "postinstall": "npm run dll",
    "postuninstall": "npm run dll"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carney520/jh-ts-react-boilerplate.git"
  },
  "keywords": [
    "react",
    "typescript",
    "mobx",
    "boilerplate"
  ],
  "author": "ivan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carney520/jh-ts-react-boilerplate/issues"
  },
  "homepage": "https://github.com/carney520/jh-ts-react-boilerplate#readme",
  "devDependencies": {
    "@types/fbemitter": "^2.0.32",
    "@types/lodash": "^4.14.72",
    "@types/react": "^16.0.22",
    "@types/react-dom": "^16.0.3",
    "@types/react-helmet": "^5.0.3",
    "@types/react-hot-loader": "^3.0.5",
    "@types/react-intl": "^2.3.2",
    "@types/react-loadable": "^4.0.4",
    "@types/react-router": "^4.0.17",
    "@types/react-router-dom": "^4.2.1",
    "@types/webpack-env": "^1.13.2",
    "autoprefixer": "^7.1.2",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.1.0",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "fork-ts-checker-webpack-plugin": "^0.2.9",
    "fuzzy": "^0.1.3",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "husky": "^0.14.3",
    "inquirer-autocomplete-prompt": "^0.11.1",
    "inquirer-recursive": "^0.0.5",
    "jh-tslint": "^1.0.0",
    "lint-staged": "^4.0.3",
    "object-assign": "^4.1.1",
    "plop": "^1.8.0",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.8",
    "prettier": "^1.8.2",
    "react-dev-utils": "^4.2.1",
    "react-hot-loader": "next",
    "shelljs": "^0.7.8",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-processor-styled-components": "^1.0.0",
    "svg-fill-loader": "^0.0.8",
    "svg-sprite-loader": "^3.2.2",
    "svgo-loader": "^1.2.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "ts-loader": "^3.1.1",
    "tslint": "^5.8.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-manifest-plugin": "^1.2.1"
  },
  "dependencies": {
    "fbemitter": "^2.1.1",
    "intl": "^1.2.5",
    "lodash": "^4.17.4",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.4",
    "mobx-react-router": "^4.0.1",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-loadable": "^5.3.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "sanitize.css": "^5.0.0",
    "styled-components": "^2.2.3",
    "tslib": "^1.8.0"
  },
  "allowedEnvs": [],
  "dllPlugin": {
    "exclude": [
      "/^@types/",
      "sanitize.css"
    ],
    "include": []
  }
}
